<div class="row">

  <div class="col-lg-6">
    <h2>Delete cached data</h2>
    <form id="insert" action="/delete" method="post">
      <div class="form-group">
        <div class="input-group">
          <span class="input-group-addon" id="basic-addon1">Key</span>
          <input type="text" name="key" value="" class="form-control">
        </div>
      </div>
      <div class="form-group">
        <input type="submit" name="submit" value="Insert" class="btn btn-primary">
      </div>
    </form>
  </div>

  <div class="col-lg-6">
    <h3>Result</h3>
    <p>
      <%= @result %>
    </p>
  </div>
</div>
